{
  "$schema": "http://json.schemastore.org/template",
  "author": "RicherTunes",
  "classifications": ["Lidarr", "Plugin", "Streaming"],
  "identity": "RicherTunes.Lidarr.Plugin.Template.CSharp",
  "name": "Lidarr Streaming Plugin",
  "shortName": "lidarr-plugin",
  "sourceName": "MyServiceArr",
  "preferNameDirectory": true,
  "symbols": {
    "ServiceId": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "MyService",
      "description": "Short service identifier (e.g., Qobuz, TIDAL)"
    }
  },
  "primaryOutputs": [
    { "path": "src/MyServiceArr/MyServiceArr.csproj" }
  ]
}

